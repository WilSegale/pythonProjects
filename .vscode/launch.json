
{
     "[python]": {
          "editor.defaultFormatter": "ms-python.autopep8",
          "editor.formatOnSave": true
     },
      
     "version": "0.2.0",
     "configurations": [
          {
               "args": [
                    "--extensionDevelopmentPath=${workspaceFolder}"
               ],
               "name": "Launch Extension",
               "outFiles": [
                    "${workspaceFolder}/out/**/*.js"
               ],
               "preLaunchTask": "npm",
               "request": "launch",
               "type": "extensionHost"
          },
          {
               "command": "npm start",
               "name": "Run npm start",
               "request": "launch",
               "type": "node-terminal"
          },
          {
               "name": "Attach",
               "port": 9229,
               "request": "attach",
               "skipFiles": [
                    "<node_internals>/**"
               ],
               "type": "node"
          },
          {
               "name": "Attach to Chrome",
               "port": 9222,
               "request": "attach",
               "type": "chrome",
               "webRoot": "${workspaceFolder}"
          },
          {
               "name": "Python: Attach using Process Id",
               "type": "python",
               "request": "attach",
               "processId": "${command:pickProcess}",
               "justMyCode": true
          },
          {
               "name": "Python: FastAPI",
               "type": "python",
               "request": "launch",
               "module": "uvicorn",
               "args": [
                    "main:app"
               ],
               "jinja": true,
               "justMyCode": true
          },
          {
               "type": "java",
               "name": "Spring Boot-MysqlTestApplication<spring-petclinic>",
               "request": "launch",
               "cwd": "${workspaceFolder}",
               "mainClass": "org.springframework.samples.petclinic.MysqlTestApplication",
               "projectName": "spring-petclinic",
               "args": "",
               "envFile": "${workspaceFolder}/.env"
          }
     ]
}